{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eltazr3-adf"
		},
		"ls_storage_curated_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_storage_curated'"
		},
		"ls_syn_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_syn_sql'"
		},
		"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://eltazr3-syn.dev.azuresynapse.net"
		},
		"AzureSynapseArtifacts2_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://eltazr3-syn.dev.azuresynapse.net"
		},
		"ls_adls_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eltazr3adls.dfs.core.windows.net/"
		},
		"ls_adls_raw_adf_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eltazr3adls.dfs.core.windows.net/"
		},
		"ls_syn_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eltazr3-syn.sql.azuresynapse.net"
		},
		"ls_syn_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "eltazr3_sqlpool"
		},
		"ls_syn_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "synadmin"
		},
		"t_daily_nyctaxi_properties_pipeline_parameters_dataset": {
			"type": "string",
			"defaultValue": "nyctaxi_yellow"
		},
		"t_daily_nyctaxi_properties_pipeline_parameters_run_date": {
			"type": "string",
			"defaultValue": "2025-07-31"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eltazr3-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_storage_curated_connectionString": {
						"value": "[parameters('ls_storage_curated_connectionString')]"
					},
					"ls_syn_sql_password": {
						"value": "[parameters('ls_syn_sql_password')]"
					},
					"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureSynapseArtifacts1_properties_typeProperties_endpoint')]"
					},
					"AzureSynapseArtifacts2_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureSynapseArtifacts2_properties_typeProperties_endpoint')]"
					},
					"ls_adls_raw_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_raw_properties_typeProperties_url')]"
					},
					"ls_adls_raw_adf_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_raw_adf_properties_typeProperties_url')]"
					},
					"ls_syn_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_syn_sql_properties_typeProperties_server')]"
					},
					"ls_syn_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_syn_sql_properties_typeProperties_database')]"
					},
					"ls_syn_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_syn_sql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "eltazr3-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"t_daily_nyctaxi_properties_pipeline_parameters_dataset": {
						"value": "[parameters('t_daily_nyctaxi_properties_pipeline_parameters_dataset')]"
					},
					"t_daily_nyctaxi_properties_pipeline_parameters_run_date": {
						"value": "[parameters('t_daily_nyctaxi_properties_pipeline_parameters_run_date')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'eltazr3-adf_ArmTemplate_0')]"
			]
		}
	]
}