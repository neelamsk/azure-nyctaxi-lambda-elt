{
	"name": "pl_raw_to_stg_nyctaxi",
	"properties": {
		"activities": [
			{
				"name": "DeleteStagingPartition",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "ls_syn_sql",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "DELETE FROM stg.trip WHERE ingest_date = '@{pipeline().parameters.run_date}';\n"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			}
		],
		"parameters": {
			"dataset": {
				"type": "string",
				"defaultValue": "nyctaxi_yellow"
			},
			"run_date": {
				"type": "string",
				"defaultValue": "@formatDateTime(utcnow(),'yyyy-MM-dd')"
			},
			"file_name": {
				"type": "string"
			},
			"run_id": {
				"type": "string",
				"defaultValue": "@pipeline().RunId"
			}
		},
		"annotations": []
	}
}